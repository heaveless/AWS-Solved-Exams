    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "serverless": "^3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-dynamodb": "^0.2.55",
    "serverless-esbuild": "^1.54.5",
    "serverless-offline": "^13.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-tscpaths": "^0.0.8",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.6.3"
